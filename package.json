  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "destroy": "rimraf server_prod",
    "postinstall": "npm run destroy && mkdir server_prod && npm run build && npm run babel_prod",
    "babel_prod": "babel server -s -d server_prod",
    "babel": "babel server -s -d server_prod --watch",
    "watch": "concurrently \"npm run babel\" \"nodemon server\""
